#pragma once

class Element {

  int value;
  Element* parent;
  Element* left;
  Element* right;
  friend class Lista;
  friend class Stos;
  friend class Kolejka;
  friend class Drzewo;
public:
  int getValue();
  Element* getNext();
  Element* getPrev();
  void setValue(int);
  void setNext(Element*);
  void setPrev(Element*);
};
